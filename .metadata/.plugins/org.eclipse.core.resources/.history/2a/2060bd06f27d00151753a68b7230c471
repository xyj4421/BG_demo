	function removeTreeNode() {
		var node = $('#type_tree').tree('getSelected');
		if (node) {
			$.messager.confirm('ÌáÊ¾',
					'ç¡®è®¤åˆ é™¤å—ï¼Ÿ', function(
							r) {
						if (r) {
							//alert("aaaaaaaaaaS");
							$.post('deleteNode', {
								id : node.id
							}, function(result) {
							$('#type_tree').tree('reload');
								
							});
						}
					});
		}
	}